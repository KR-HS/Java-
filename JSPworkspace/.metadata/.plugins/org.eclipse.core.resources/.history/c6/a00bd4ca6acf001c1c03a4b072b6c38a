<%@page import="java.time.LocalDate"%>
<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>만년달력 만들기 01</title>
<%
	// 년도와 월을 받는다.
	int year=LocalDate.now().getYear();
	int month=LocalDate.now().getMonthValue();
	
%>
</head>
<body>
	<h1><%= year %>년 <%month %>월</h1>
</body>
</html>