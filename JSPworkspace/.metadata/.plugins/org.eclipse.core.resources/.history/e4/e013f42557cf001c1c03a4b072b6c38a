	<%
		int n=0;
		try{
			int n=Integer.parseInt(request.getParameter("n"));
		}catch(Exception e){
			;
		}
		
		int sum=0;
		for(int i=1;i<=n;i++) sum+=i;
	
	%>
	
	1에서 <%=n %>까지의 합 : <%=sum %><br/>
